<div class="span2">
  <div class="sidebar-nav">
    <div class="profile-box">
      <h5>
        <%= this.model.get('first_name') %>
      </h5>
      <img src="
        <%= this.model.get('avatar_thumb') %>
        " alt="profile-pic" class="profile-pic" />
      <a href="#">Edit Profile</a>  
    </div>
    <ul class="nav nav-tabs nav-stacked">
      <li class="nav-header">Navigation</li>
      <li><a href="#places" class="toVisit">To Visit</a></li>
      <li><a href="#visited" class="visited">Visited</a></li>
      <li class="nav-header">Functions</li>
      <li><a href="#new" class="addNewPlace">Add a New Place</a></li>
    </ul>
    <div class="tag_list">
      <% for (var i=0; i < this.tags.length; i++) { %>
        <span class="tag"><%= this.tags[i].name %></span>
      <% } %>
    </div>
  </div>
</div>
